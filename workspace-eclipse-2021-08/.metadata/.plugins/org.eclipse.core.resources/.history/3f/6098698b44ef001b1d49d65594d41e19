package kr.co.hanshinit.NeoCMS.cop.sms.service.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import egovframework.rte.psl.dataaccess.EgovAbstractDAO;
import kr.co.hanshinit.NeoCMS.cop.sms.service.MmsVO;
import kr.co.hanshinit.NeoCMS.cop.sms.service.MsgDataVO;

@Repository("msgDataDAO")
public class MsgDataDAO extends EgovAbstractDAO {

	@SuppressWarnings("unchecked")
	public List<MsgDataVO> selectMsgDataList(MsgDataVO msgDataVO) {
		return (List<MsgDataVO>) list("msgDataDAO.selectMsgDataList", msgDataVO);
	}

	public MsgDataVO selectMsgDataData(MsgDataVO msgDataVO) {
		return (MsgDataVO) select("msgDataDAO.selectMsgDataData", msgDataVO);
	}

	public Integer selectMsgDataTotCnt(MsgDataVO msgDataVO) {
		return (Integer) select("msgDataDAO.selectMsgDataTotCnt", msgDataVO);
	}

	public void insertMmsData(MmsVO mmsVO) {
		insert("msgDataDAO.insertMmsData", mmsVO);
	}

	public void insertMsgData(List<MsgDataVO> msgDataList) {
		insert("msgDataDAO.insertMsgDataListIterator", msgDataList);
	}

	public void updateMsgData(MsgDataVO msgDataVO) {
		update("msgDataDAO.updateMsgData", msgDataVO);
	}

	public void deleteMsgData(MsgDataVO msgDataVO) {
		delete("msgDataDAO.deleteMsgData", msgDataVO);
	}

	@SuppressWarnings("unchecked")
	public List<MsgDataVO> selectMsgDataResult(MsgDataVO msgDataVO) {
		return (List<MsgDataVO>) list("msgDataDAO.selectMsgDataResult", msgDataVO);
	}

	public Integer selectMsgDataToCount(MsgDataVO msgDataVO) {
		return (Integer) select("msgDataDAO.selectMsgDataToCount", msgDataVO);
	}

	public Integer selectMmsContSeq(MmsVO mmsVO) {
		return (Integer) select("msgDataDAO.selectMmsContSeq", mmsVO);
	}

	public void updateMmsData(MmsVO mmsVO) {
		update("msgDataDAO.updateMmsData", mmsVO);
	}

	public void deleteMmsData(MmsVO mmsVO) {
		delete("msgDataDAO.deleteMmsData", mmsVO);
	}

	public void mmsSendComp(MmsVO mmsVO) {
		update("msgDataDAO.mmsSendComp", mmsVO);
	}

/*	@SuppressWarnings("unchecked")
	public List<MsgDataVO> selectMsgCountList(MsgDataVO msgDataVO) {
		return (List<MsgDataVO>) list("msgDataDAO.selectMsgCountList", msgDataVO);
	}*/

	public void insertMsgDataStaff(List<MsgDataVO> msgDataList) {
		insert("msgDataDAO.insertMsgDataStaff", msgDataList);
	}

	public void insertMsgDataResve(List<MsgDataVO> msgDataList) {
		insert("msgDataDAO.insertMsgDataResve", msgDataList);
	}


	public MsgDataVO selectSmsSentRsltInfo(MsgDataVO msgDataVO) {
		return (MsgDataVO) select("msgDataDAO.selectSmsSentRsltInfo", msgDataVO);
	}
	@SuppressWarnings("unchecked")
	public List<MsgDataVO> selectSmsSentFailList(MsgDataVO msgDataVO) {
		return (List<MsgDataVO>) list("msgDataDAO.selectSmsSentFailList", msgDataVO);
	}


	public int deleteMsgDataAll(MsgDataVO msgDataVO) throws Exception
	{
		return delete("msgDataDAO.deleteMsgDataAll", msgDataVO);
	}

}

