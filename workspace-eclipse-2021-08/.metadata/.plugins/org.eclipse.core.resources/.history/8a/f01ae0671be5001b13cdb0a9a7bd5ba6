public class FbApiKey extends CmmModel {
  private static final long serialVersionUID = -154239255659793176L;
  
  //  API KEY 관리번호
  private int keyNo;
  
  //  API KEY 관리 명
  private String mngNm;
  
  //  FACEBOOK ID
  private String fbId;
  
  //  FACEBOOK APP ID
  private String fbAppId;
  
  //  FACEBOOK APP SECRET CODE;
  private String fbAppScrtCd;
  
  //  FACEBOOK API TEMP TOKEN
  private String fbTmpToken;
  
  //  FACEBOOK API LONG TERM TOKEN
  private String fbLngTmToken;
  
  //  EXPIRE DATETIME OF FACEBOOK API LONG TERM TOKEN
  private String exprTm;
  
  //  MANAGER E-MAIL
  private String mngrEmail;
  
  //  flag
  private String flag;
}
