import com.koem.vms.waterdepth.dto.WaterDepthDTO;

@Repository
public interface WaterDepthDAO {

  List<WaterDepthDTO> selectAll() throws Exception;
  List<WaterDepthDTO> getWaterDepthInRage( String range ) throws Exception;
}
