	void deleteMsgData(MsgDataVO msgDataVO) throws Exception;

	void insertMmsData(MmsVO mmsVO) throws Exception;

	int selectMmsContSeq(MmsVO mmsVO) throws Exception;

	void updateMmsData(MmsVO mmsVO) throws Exception;

	void deleteMmsData(MmsVO mmsVO) throws Exception;

	void mmsSendComp(MmsVO mmsVO) throws Exception;

	//List<MsgDataVO> selectMsgCountList(MsgDataVO msgDataVO) throws Exception;

